motif ExtractXwithReferenceClass
select OrigContainer, Extract, NewContainer, ContainerContainer, MovedReference
	where "OrigContainer.getEClass().isAbstract() == false" 
	and "NewContainer.getEClass().isAbstract() == false"  
	and "ContainerContainer.getEClass().isAbstract() == false" 
	and "MovedReference.getEClass().isAbstract() == false"
connected by extracts(OrigContainer>Extract)[1,*] find all
	where "extracts is org.modelrefactoring.guery.graph.ContainmentEdge"
connected by referer(OrigContainer>MovedReference)[1,*] find all
	where "referer is org.modelrefactoring.guery.graph.ContainmentEdge"
connected by moved(NewContainer>Extract)[1,*] find all
	where "moved is org.modelrefactoring.guery.graph.ContainmentEdge"
connected by target(ContainerContainer>NewContainer)[1,*] find all 
	where "target is org.modelrefactoring.guery.graph.ContainmentEdge"
connected by source(ContainerContainer>OrigContainer)[1,*] find all
	where "source is org.modelrefactoring.guery.graph.ContainmentEdge"
connected by containerRef(MovedReference>NewContainer)[1,*] find all

