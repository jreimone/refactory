RoleModel ExtractMethod {
	input runtime input runtime optional ROLE OrigContainer;
	ROLE Extract;
	ROLE NewContainer;
	ROLE ContainerContainer;
	ROLE MovedReference;
	
	OrigContainer [1..1] <>- Extract extracts [0..-1] (transitive);
	OrigContainer [1..1] <>- MovedReference [1..1] (transitive);
	
	NewContainer [1..1] <>- Extract moved [0..-1] (transitive);
	
	ContainerContainer [1..1] <>- NewContainer [1..1] (transitive);
	ContainerContainer [1..1] <>- OrigContainer [1..1] (transitive reflexiv);
	
	MovedReference [1..1] -- NewContainer [1..1];
} 