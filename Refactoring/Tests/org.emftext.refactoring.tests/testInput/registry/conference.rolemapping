ROLEMODELMAPPING conference FOR <http://www.emftext.org/language/conference> 

ExtractTrack maps <ExtractMethod> { 
	Extract := Slot;
	OrigContainer := Track{
		extracts := slots;
	};
	NewContainer := Track (newName -> name){
		moved := slots;
	};
	ContainerContainer := Conference{
		source := elements:Track;
		target := elements:Track;
	};
} 

CreateTrackTest maps <platform:/resource/org.emftext.refactoring.tests/testInput/registry/CREATEcommand.rolestext> {
	Container := Conference;
	Createe := Track;
}	
	