<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.emftext.access.syntax">
		<metaInformationProvider class="org.emftext.language.refactoring.roles.resource.rolestext.mopp.RolestextMetaInformation" id="org.emftext.language.refactoring.roles.resource.rolestext.mopp.RolestextMetaInformation">
		</metaInformationProvider>
	</extension>
	
	<extension point="org.eclipse.ui.editors">
		<editor class="org.emftext.language.refactoring.roles.resource.rolestext.ui.RolestextEditor" contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor" extensions="rolestext" icon="icons/editor_icon.gif" id="org.emftext.language.refactoring.roles.resource.rolestext.ui.RolestextEditor" name="EMFText Role Model Editor">
		</editor>
	</extension>
	
	<extension point="org.eclipse.core.runtime.preferences">
		<initializer class="org.emftext.language.refactoring.roles.resource.rolestext.ui.RolestextPreferenceInitializer">
		</initializer>
	</extension>
	
	<extension point="org.eclipse.ui.preferencePages">
		<page name="Role Model Text Editor" id="org.emftext.language.refactoring.roles.resource.rolestext.ui.RolestextPreferencePage" class="org.emftext.language.refactoring.roles.resource.rolestext.ui.RolestextPreferencePage" category="org.eclipse.ui.preferencePages.GeneralTextEditor">
		</page>
		<page name="Syntax Coloring" id="org.emftext.language.refactoring.roles.resource.rolestext.ui.RolestextSyntaxColoringPreferencePage" class="org.emftext.language.refactoring.roles.resource.rolestext.ui.RolestextSyntaxColoringPreferencePage" category="org.emftext.language.refactoring.roles.resource.rolestext.ui.RolestextPreferencePage">
		</page>
		<page name="Brackets" id="org.emftext.language.refactoring.roles.resource.rolestext.ui.RolestextBracketPreferencePage" class="org.emftext.language.refactoring.roles.resource.rolestext.ui.RolestextBracketPreferencePage" category="org.emftext.language.refactoring.roles.resource.rolestext.ui.RolestextPreferencePage">
		</page>
		<page name="Occurrence" id="org.emftext.language.refactoring.roles.resource.rolestext.ui.RolestextOccurrencePreferencePage" class="org.emftext.language.refactoring.roles.resource.rolestext.ui.RolestextOccurrencePreferencePage" category="org.emftext.language.refactoring.roles.resource.rolestext.ui.RolestextPreferencePage">
		</page>
	</extension>
	
	<extension point="org.eclipse.ui.newWizards">
		<category id="org.emftext.runtime.ui.EMFTextFileCategory" name="EMFText File">
		</category>
		<wizard category="org.emftext.language.refactoring.remfactor" icon="/icons/default_new_icon.gif" class="org.emftext.language.refactoring.roles.resource.rolestext.ui.RolestextNewFileWizard" id="org.emftext.language.refactoring.roles.resource.rolestext.ui.RolestextNewFileWizard" name="EMFText Role Model (.rolestext) file">
		</wizard>
	</extension>
	
	<extension id="org.emftext.language.refactoring.roles.resource.rolestext.problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.eclipse.core.resources.problemmarker">
		</super>
		<super type="org.eclipse.core.resources.textmarker">
		</super>
	</extension>
	
	<extension id="org.emftext.language.refactoring.roles.resource.rolestext.nature" name="rolestext nature" point="org.eclipse.core.resources.natures">
		<runtime>
			<run class="org.emftext.language.refactoring.roles.resource.rolestext.mopp.RolestextNature" />
		</runtime>
		<builder id="org.emftext.language.refactoring.roles.resource.rolestext.builder" />
	</extension>
	
	<extension point="org.eclipse.core.resources.builders" id="org.emftext.language.refactoring.roles.resource.rolestext.builder" name="rolestext Builder">
		<builder hasNature="true">
			<run class="org.emftext.language.refactoring.roles.resource.rolestext.mopp.RolestextBuilderAdapter" />
		</builder>
	</extension>
	
	<extension-point id="org.emftext.language.refactoring.roles.resource.rolestext.default_load_options" name="Default Load Options" schema="schema/default_load_options.exsd"/>
	
	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser class="org.emftext.language.refactoring.roles.resource.rolestext.mopp.RolestextResourceFactoryDelegator" type="rolestext">
		</parser>
	</extension>
	
	<extension-point id="org.emftext.language.refactoring.roles.resource.rolestext.additional_extension_parser" name="Additional Extension Parser" schema="schema/additional_extension_parser.exsd"/>
	
</plugin>
