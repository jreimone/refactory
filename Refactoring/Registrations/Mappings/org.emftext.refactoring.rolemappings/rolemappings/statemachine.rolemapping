ROLEMODELMAPPING statemachine FOR <http://www.eclipse.org/uml2/3.0.0/UML> 

ExtractCompositeState maps <ExtractXwithReferenceClass> {  
	OrigContainer := Region{
		extracts := subvertex:State;
		referer := transition;
	};
	
	Extract := State;
	
	ContainerContainer := Region{
		target := subvertex:State;
	};
	
	NewContainer := State (newName -> name){
		moved := region -> subvertex:State;
	};
}	

RenameElement maps <RenameX> {
	Nameable := NamedElement (name -> name);
}