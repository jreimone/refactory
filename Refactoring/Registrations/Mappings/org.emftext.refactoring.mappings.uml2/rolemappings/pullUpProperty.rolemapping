ROLEMODELMAPPING FOR <http://www.eclipse.org/uml2/3.0.0/UML>

/**
 * Pull Up Property - removes a property from a class and 
 * inserts it into one of its super-classes.
 */
PullUpProperty maps <MoveX> {

	Movee := Property;
	SourceContainer := Class {
		sourceContainment := ownedAttribute;
		containerReference := redefinedClassifier;
	};
	TargetContainer := Class {
		targetContainment := ownedAttribute;
	};
} 
