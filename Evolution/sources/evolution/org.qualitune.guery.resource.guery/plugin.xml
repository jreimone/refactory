<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.emftext.access.syntax">
		<metaInformationProvider class="org.qualitune.guery.resource.guery.mopp.GueryMetaInformation" id="org.qualitune.guery.resource.guery.mopp.GueryMetaInformation">
		</metaInformationProvider>
	</extension>
	
	<extension id="org.qualitune.guery.resource.guery.problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.eclipse.core.resources.problemmarker">
		</super>
		<super type="org.eclipse.core.resources.textmarker">
		</super>
		<super type="org.eclipse.emf.ecore.diagnostic">
		</super>
	</extension>
	
	<extension id="org.qualitune.guery.resource.guery.problem.syntax_error" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.qualitune.guery.resource.guery.problem">
		</super>
	</extension>
	
	<extension id="org.qualitune.guery.resource.guery.problem.print_problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.qualitune.guery.resource.guery.problem">
		</super>
	</extension>
	
	<extension id="org.qualitune.guery.resource.guery.problem.unresolved_reference" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.qualitune.guery.resource.guery.problem">
		</super>
	</extension>
	
	<extension id="org.qualitune.guery.resource.guery.problem.analysis_problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.qualitune.guery.resource.guery.problem">
		</super>
	</extension>
	
	<extension id="org.qualitune.guery.resource.guery.problem.builder_error" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.qualitune.guery.resource.guery.problem">
		</super>
	</extension>
	
	<extension id="org.qualitune.guery.resource.guery.nature" name="guery nature" point="org.eclipse.core.resources.natures">
		<runtime>
			<run class="org.qualitune.guery.resource.guery.mopp.GueryNature">
			</run>
		</runtime>
		<builder id="org.qualitune.guery.resource.guery.builder">
		</builder>
	</extension>
	
	<extension point="org.eclipse.core.resources.builders" id="org.qualitune.guery.resource.guery.builder" name="guery Builder">
		<builder hasNature="true">
			<run class="org.qualitune.guery.resource.guery.mopp.GueryBuilderAdapter">
			</run>
		</builder>
	</extension>
	
	<extension-point id="org.qualitune.guery.resource.guery.default_load_options" name="Default Load Options" schema="schema/default_load_options.exsd">
	</extension-point>
	
	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser class="org.qualitune.guery.resource.guery.mopp.GueryResourceFactoryDelegator" type="guery">
		</parser>
	</extension>
	
	<extension-point id="org.qualitune.guery.resource.guery.additional_extension_parser" name="Additional Extension Parser" schema="schema/additional_extension_parser.exsd">
	</extension-point>
	
</plugin>
