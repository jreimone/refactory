ROLEMODELMAPPING FOR <http://www.eclipse.org/uml2/3.0.0/UML>

/**
 * Pull Up Operation To Superinterface - moves an operation 
 * of an interface to one of its superinterfaces.
 */
"Pull Up Operation To Super Interface" maps <MoveX> {

	Movee := Operation;
	SourceContainer := Interface {
		sourceContainment := ownedElement;
		containerReference := redefinedElement;
	};
	TargetContainer := Interface {
		targetContainment := ownedElement;
	};
} 
