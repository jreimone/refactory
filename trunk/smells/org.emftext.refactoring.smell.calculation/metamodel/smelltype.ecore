<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="smelltype" nsURI="http://modelrefactoring.org/smell/type" nsPrefix="smelltype">
  <eClassifiers xsi:type="ecore:EClass" name="SmellType" abstract="true">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="lowerBound" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EFloat">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Defines the lower bound of the value range of this type. Must be greater or equal to 0 and less than upperBound."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="upperBound" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EFloat">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Defines the upper bound of the value range of this type. Must be greater or equal to 0 and greater than lowerBound. If upperBound is -1 then the value range is infinite."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="increment" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EFloat">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Specifies the value which will be continuously added to lowerBound until upperBound is reached. Value must be greater than 0. If the value is -1 than an infinite value range is assumed.&#xD;&#xA;&lt;br>&lt;br>&#xD;&#xA;This way the value range can be counted.&#xD;&#xA;&lt;br>&lt;br>&#xD;&#xA;Example:&lt;br>&#xD;&#xA;lowerBound = 0&lt;br>&#xD;&#xA;upperBound = 1&lt;br>&#xD;&#xA;increment = 1&lt;br>&lt;br>&#xD;&#xA;Results in the boolean type since it has only two values: 0 and 1."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="SmellTypeModel">
    <eStructuralFeatures xsi:type="ecore:EReference" name="types" upperBound="-1"
        eType="#//SmellType" containment="true"/>
  </eClassifiers>
</ecore:EPackage>
