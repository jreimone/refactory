<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            class="org.emftext.refactoring.ui.RefactoringMenuContributor"
            locationURI="popup:org.eclipse.ui.popup.any?after=additions">
      </menuContribution>
   </extension>
<extension
         point="org.eclipse.ltk.ui.refactoring.changePreviewViewers">
      <changePreviewViewer
            class="org.emftext.refactoring.ltk.ModelChangePreviewer"
            id="org.emftext.refactoring.ltk.modelChangePreviewer">
         <enablement>
            <instanceof
                  value="org.emftext.refactoring.ltk.ModelRefactoringChange">
            </instanceof></enablement>
      </changePreviewViewer>
   </extension>
</plugin>
