<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.emftext.access.syntax">
		<metaInformationProvider class="org.emftext.language.refactoring.rolemapping.resource.rolemapping.mopp.RolemappingMetaInformation" id="org.emftext.language.refactoring.rolemapping.resource.rolemapping.mopp.RolemappingMetaInformation">
		</metaInformationProvider>
	</extension>
	
	<extension point="org.eclipse.ui.editors">
		<editor class="org.emftext.language.refactoring.rolemapping.resource.rolemapping.ui.RolemappingEditor" contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor" extensions="rolemapping" icon="icons/editor_icon.gif" id="org.emftext.language.refactoring.rolemapping.resource.rolemapping.ui.RolemappingEditor" name="EMFText rolemapping Editor">
		</editor>
	</extension>
	
	<extension point="org.eclipse.core.runtime.preferences">
		<initializer class="org.emftext.language.refactoring.rolemapping.resource.rolemapping.ui.RolemappingPreferenceInitializer">
		</initializer>
	</extension>
	
	<extension point="org.eclipse.ui.preferencePages">
		<page name="Rolemapping Text Editor" id="org.emftext.language.refactoring.rolemapping.resource.rolemapping.ui.RolemappingPreferencePage" class="org.emftext.language.refactoring.rolemapping.resource.rolemapping.ui.RolemappingPreferencePage" category="org.eclipse.ui.preferencePages.GeneralTextEditor">
		</page>
		<page name="Syntax Coloring" id="org.emftext.language.refactoring.rolemapping.resource.rolemapping.ui.RolemappingSyntaxColoringPreferencePage" class="org.emftext.language.refactoring.rolemapping.resource.rolemapping.ui.RolemappingSyntaxColoringPreferencePage" category="org.emftext.language.refactoring.rolemapping.resource.rolemapping.ui.RolemappingPreferencePage">
		</page>
		<page name="Brackets" id="org.emftext.language.refactoring.rolemapping.resource.rolemapping.ui.RolemappingBracketPreferencePage" class="org.emftext.language.refactoring.rolemapping.resource.rolemapping.ui.RolemappingBracketPreferencePage" category="org.emftext.language.refactoring.rolemapping.resource.rolemapping.ui.RolemappingPreferencePage">
		</page>
		<page name="Occurrence" id="org.emftext.language.refactoring.rolemapping.resource.rolemapping.ui.RolemappingOccurrencePreferencePage" class="org.emftext.language.refactoring.rolemapping.resource.rolemapping.ui.RolemappingOccurrencePreferencePage" category="org.emftext.language.refactoring.rolemapping.resource.rolemapping.ui.RolemappingPreferencePage">
		</page>
	</extension>
	
	<extension point="org.eclipse.ui.newWizards">
		<category id="org.emftext.runtime.ui.EMFTextFileCategory" name="EMFText File">
		</category>
		<wizard category="org.emftext.runtime.ui.EMFTextFileCategory" icon="/icons/default_new_icon.gif" class="org.emftext.language.refactoring.rolemapping.resource.rolemapping.ui.RolemappingNewFileWizard" id="org.emftext.language.refactoring.rolemapping.resource.rolemapping.ui.RolemappingNewFileWizard" name="EMFText .rolemapping file">
		</wizard>
	</extension>
	
	<extension id="org.emftext.language.refactoring.rolemapping.resource.rolemapping.problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.eclipse.core.resources.problemmarker">
		</super>
		<super type="org.eclipse.core.resources.textmarker">
		</super>
	</extension>
	
	<extension id="org.emftext.language.refactoring.rolemapping.resource.rolemapping.nature" name="rolemapping nature" point="org.eclipse.core.resources.natures">
		<runtime>
			<run class="org.emftext.language.refactoring.rolemapping.resource.rolemapping.mopp.RolemappingNature" />
		</runtime>
		<builder id="org.emftext.language.refactoring.rolemapping.resource.rolemapping.builder" />
	</extension>
	
	<extension point="org.eclipse.core.resources.builders" id="org.emftext.language.refactoring.rolemapping.resource.rolemapping.builder" name="rolemapping Builder">
		<builder hasNature="true">
			<run class="org.emftext.language.refactoring.rolemapping.resource.rolemapping.mopp.RolemappingBuilderAdapter" />
		</builder>
	</extension>
	
	<extension-point id="org.emftext.language.refactoring.rolemapping.resource.rolemapping.default_load_options" name="Default Load Options" schema="schema/default_load_options.exsd"/>
	
	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser class="org.emftext.language.refactoring.rolemapping.resource.rolemapping.mopp.RolemappingResourceFactoryDelegator" type="rolemapping">
		</parser>
	</extension>
	
	<extension-point id="org.emftext.language.refactoring.rolemapping.resource.rolemapping.additional_extension_parser" name="Additional Extension Parser" schema="schema/additional_extension_parser.exsd"/>
	
</plugin>
