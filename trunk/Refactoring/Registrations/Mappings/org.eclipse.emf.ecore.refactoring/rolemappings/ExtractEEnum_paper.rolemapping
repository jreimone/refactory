ROLEMODELMAPPING extractEEnumMapping
FOR <http://www.eclipse.org/emf/2002/Ecore> 

ExtractEEnum 
	maps <ExtractXwithReference> {
	Extractee := EEnumLiteral;
	OrigContainer := EEnum {
		extracts := eLiterals;
		reference := ePackage;
	};
	NewContainer := EEnum(newName -> name){
		moved := eLiterals;
	};
	ContainerContainer := EPackage {
		source := eClassifiers;
		target := eClassifiers;
	};
}