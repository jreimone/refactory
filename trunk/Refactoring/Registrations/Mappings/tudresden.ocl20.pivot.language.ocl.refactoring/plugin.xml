<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.emftext.refactoring.rolemapping">
      <rolemapping
            rolemapping_resource="rolemappings/renamings.rolemapping">
      </rolemapping>
   </extension>
   <extension
         point="org.emftext.refactoring.rolemapping">
      <rolemapping
            rolemapping_resource="rolemappings/extractings.rolemapping">
      </rolemapping>
   </extension>
   <extension
         point="org.emftext.refactoring.rolemapping">
      <rolemapping
            rolemapping_resource="rolemappings/inlinings.rolemapping">
      </rolemapping>
   </extension>
   <extension
         point="org.emftext.refactoring.postprocessor">
      <postprocessor
            mapping_name="RenameSimpleNameCS"
            nsUri="http://www.tu-dresden.de/ocl20/pivot/language/ocl"
            postprocessor="oclrefactoring.OCLPostProcessor">
      </postprocessor>
   </extension>
   <extension
         point="org.emftext.refactoring.postprocessor">
      <postprocessor
            mapping_name="Extract Let Expression"
            nsUri="http://www.tu-dresden.de/ocl20/pivot/language/ocl"
            postprocessor="oclrefactoring.ExLetExpPostProcessor">
      </postprocessor>
   </extension>
   <extension
         point="org.emftext.refactoring.postprocessor">
      <postprocessor
            mapping_name="Inline Variable From LetExpression"
            nsUri="http://www.tu-dresden.de/ocl20/pivot/language/ocl"
            postprocessor="oclrefactoring.InlineVarFromLetExp">
      </postprocessor>
   </extension>
   <extension
         point="org.emftext.refactoring.postprocessor">
      <postprocessor
            mapping_name="Remove All Empty Variable Declarations"
            nsUri="http://www.tu-dresden.de/ocl20/pivot/language/ocl"
            postprocessor="oclrefactoring.RemoveAllEmptyVariableDeclarationsPostProcessor">
      </postprocessor>
   </extension>
   <extension
         point="org.emftext.refactoring.postprocessor">
      <postprocessor
            mapping_name="Remove Redundant Brackets"
            nsUri="http://www.tu-dresden.de/ocl20/pivot/language/ocl"
            postprocessor="oclrefactoring.RemoveRedundantBrackets">
      </postprocessor>
   </extension>
   <extension
         point="org.emftext.refactoring.postprocessor">
      <postprocessor
            mapping_name="Remove All Redundant Brackets"
            nsUri="http://www.tu-dresden.de/ocl20/pivot/language/ocl"
            postprocessor="oclrefactoring.RemoveAllRedundantBrackets">
      </postprocessor>
   </extension>
   <extension
         point="org.emftext.refactoring.postprocessor">
      <postprocessor
            mapping_name="Remove All Unused Variable Declarations"
            nsUri="http://www.tu-dresden.de/ocl20/pivot/language/ocl"
            postprocessor="oclrefactoring.RemoveAllUnusedVariableDeclarations">
      </postprocessor>
   </extension>
   <extension
         point="org.emftext.refactoring.postprocessor">
      <postprocessor
            mapping_name="RenameNamedLiteralExpCS"
            nsUri="http://www.tu-dresden.de/ocl20/pivot/language/ocl"
            postprocessor="oclrefactoring.RenameNamedLiteralExpPostProcessor">
      </postprocessor>
   </extension>
   <extension
         point="org.emftext.refactoring.postprocessor">
      <postprocessor
            mapping_name="Remove Empty Variable Declaration"
            nsUri="http://www.tu-dresden.de/ocl20/pivot/language/ocl"
            postprocessor="oclrefactoring.RemoveEmptyVariableDeclarationPostProcessor">
      </postprocessor>
   </extension>

</plugin>
