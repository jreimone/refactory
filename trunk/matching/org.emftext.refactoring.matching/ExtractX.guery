motif ExtractX
select OrigContainer, Extractee, NewContainer, ContainerContainer
where "OrigContainer!=null"
and "Extractee!=null"
and "NewContainer!=null"
and "ContainerContainer!=null"
connected by extracts(OrigContainer>Extractee)[1,1]
connected by moved(NewContainer>Extractee)[1,1]
connected by target(ContainerContainer>NewContainer)[1,1]
connected by source(ContainerContainer>OrigContainer)[1,1]
connected by reference(OrigContainer>NewContainer)[1,1]